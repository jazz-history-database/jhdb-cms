{
  "name": "jhdb-cms",
  "description": "Content management system for the Jazz History Database",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "author": "Jazz History Database",
  "repository": {
    "type": "git",
    "url": "https://github.com/jazzhistorydatabase/jhdb-cms.git"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "@types/react": "^16.9.38",
    "axios": "^0.19.2",
    "backpack-core": "^0.8.4",
    "colors": "^1.4.0",
    "dropbox": "^4.0.30",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "firebase": "^7.15.3",
    "firebase-admin": "^8.12.1",
    "isomorphic-fetch": "^2.2.1",
    "parcel": "^1.12.4",
    "re-base": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.0",
    "simple-oauth2": "^3.4.0",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.3.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4"
  },
  "proxy": "http://localhost:8080",
  "scripts": {
    "deploy": "gcloud app deploy",
    "start": "node build/main.js",
    "dev:react": "parcel watch client/index.html --public-url ./",
    "dev:express": "cross-env BROWSER=none DEV=true npx backpack dev",
    "dev": "cross-env BROWSER=none DEV=true concurrently --kill-others --names \" PARCEL  , EXPRESS \" \"npm run dev:react\" \"npm run dev:express\"",
    "build": "backpack build && parcel build client/index.html --public-url ./",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
